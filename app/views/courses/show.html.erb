<p style="color: green"><%= notice %></p>

<%= render @course %>

<div>

  <%#=video_tag(url_for(video.file),controls: '') %>
</div>

<ul class="list-group">
  <%@course.videos.each do |video|%>
    <li class="list-group-item"><%=link_to video.title, video_path(video)%></li>
  <%end %>
</ul>


<div>
  <%= link_to "Edit this course", edit_course_path(@course) %> |
  <%= link_to "Back to courses", courses_path %>

  <%= button_to "Destroy this course", @course, method: :delete %>
</div>
